<Page x:Class="RunTrack.SystemKey"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RunTrack.View"
      mc:Ignorable="d" 
      d:Height="750" d:Width="1000"
      Title="Unique Key">

    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Foreground="#8D1D2C" Margin="20" FontSize="20" >Programm ID:</TextBlock>
            <TextBlock Foreground="#8D1D2C" Margin="20" FontSize="20" x:Name="tbKey" >N/A</TextBlock>
        </StackPanel>
    </Grid>
</Page>
