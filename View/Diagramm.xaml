<Window x:Class="Klimalauf.Diagramm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Klimalauf"
        mc:Ignorable="d"
        Title="Diagrammm" Height="450" Width="800">
    <Grid SizeChanged="Grid_SizeChanged_1">
        <Canvas x:Name="Diagrammcanvas" Margin="34,40,34,40" Background="LightGray"/>
        <Label x:Name="label" HorizontalContentAlignment="Center"  Height="35" Width="732" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="16" FontFamily="Segoe UI Emoji" Background="White" Foreground="#FF0079FF"/>
        <Canvas x:Name="canvascanvas" Margin="0,40,34,40" Width="34" HorizontalAlignment="Left"/>




    </Grid>
</Window>
